{
    "editor.codeLens": false,
    "editor.fontFamily": "Source Code Pro",
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.lineHeight": 19,
    "editor.rulers": [80, 100, 120],
    "editor.stickyTabStops": true,
    "editor.wrappingColumn": -1,
    "errorLens.gutterIconsEnabled": true,
    "extension-manager.extensions": [
        "ms-vscode.csharp",
        "donjayamanne.python",
        "lukehoban.Go",
        "msjsdiag.debugger-for-chrome",
        "johnpapa.Angular2",
        "eg2.tslint",
        "EditorConfig.EditorConfig",
        "dbankier.vscode-instant-markdown",
        "WallabyJs.wallaby-vscode",
        "tushortz.java-snippets",
        "gerane.Theme-Zenburn",
        "tushortz.java-imports-snippets"
    ],
    "files.exclude": {
        "**/.idea": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.pyc": true
    },
    "files.trimTrailingWhitespace": true,
    "javascript.validate.enable": false,
    "php.memoryLimit": "512M",
    "phpmd.rules": ".phpmd.xml",
    "python.showStartPage": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "window.newWindowDimensions": "inherit",
    "workbench.colorTheme": "Zenburn",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "workbench.iconTheme": "material-icon-theme",
    "workbench.preferredDarkColorTheme": "Zenburn",
    "workbench.startupEditor": "none"
}
