[user]
	name = Ricky Huang
  email = rickypc@users.noreply.github.com
[alias]
  l = log --graph --name-status --date=local
  co = checkout
  ci = commit
  st = status
[color]
  branch = auto
  status = auto
  diff = auto
  ui = auto
  interactive = auto
[pack]
  threads = 0
[gc]
  auto = 1000
[core]
  compression = 9
  excludesfile = ~/.gitignore
  longpaths = true
  autocrlf = false
[merge]
  renameLimit = 100000
[branch]
  autosetupmerge = true
#  autosetuprebase = always
[push]
  default = simple
