[alias]
  ad = add
  ci = commit
  co = checkout
  l = log --date=local --graph --name-status
  m = merge
  pl = pull
  ps = push
  st = status
[branch]
  autosetupmerge = true
#  autosetuprebase = always
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[core]
  autocrlf = false
  compression = 9
  excludesfile = ~/.gitignore
  longpaths = true
[gc]
  auto = 1000
[init]
  defaultBranch = master
[merge]
  renameLimit = 100000
[pack]
  threads = 0
[pull]
  rebase = false
[push]
  default = simple
[user]
  email = rickypc@users.noreply.github.com
  name = Ricky Huang
